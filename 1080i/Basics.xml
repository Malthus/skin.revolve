	<include name="DialogWindow">
        <param name="height" default="220" />
    
        <definition>
            <control type="group">
                <control type="image">
                    <posx>0</posx>
                    <posy>-40</posy>
                    <width>1920</width>
                    <height>40</height>
                    <fadetime>100</fadetime>
                    <texture>elements/shadow-upperborder.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
                <control type="image">
                    <posx>0</posx>
                    <posy>$PARAM[height]</posy>
                    <width>1920</width>
                    <height>40</height>
                    <fadetime>100</fadetime>
                    <texture>elements/shadow-lowerborder.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>

                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>1920</width>
                    <height>$PARAM[height]</height>
                    <fadetime>100</fadetime>
                    <aspectratio>stretch</aspectratio>
                    <texture colordiffuse="BackgroundColor">white.png</texture>
                </control>
            </control>
        </definition>
	</include>

	<include name="UpDownButton">
        <param name="posx" default="0" />
        <param name="posy" default="0" />
        <param name="downbuttonid" default="-" />
        <param name="upbuttonid" default="-" />
        <param name="controlid" default="-" />
        <param name="onup" default="Noop" />
        <param name="ondown" default="Noop" />
        <param name="onleft" default="Noop" />
        <param name="onright" default="Noop" />

        <definition>
            <control type="group">
                <posx>$PARAM[posx]</posx>
                <posy>$PARAM[posy]</posy>

                <control type="button" id="$PARAM[downbuttonid]">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>40</width>
                    <height>40</height>
                    <texturenofocus border="20" colordiffuse="SecondaryColor">controls/circle-triangledown.png</texturenofocus>
                    <texturefocus border="20" colordiffuse="BackgroundColor">controls/circle-triangledown.png</texturefocus>
                    <onup>$PARAM[onup]</onup>
                    <ondown>$PARAM[ondown]</ondown>
                    <onleft>$PARAM[onleft]</onleft>
                    <onright>$PARAM[upbuttonid]</onright>
                    <onclick>Control.Move($PARAM[controlid],1)</onclick>
                </control>
                
                <control type="button" id="$PARAM[upbuttonid]">
                    <posx>40</posx>
                    <posy>0</posy>
                    <width>40</width>
                    <height>40</height>
                    <texturenofocus border="20" colordiffuse="SecondaryColor">controls/circle-triangleup.png</texturenofocus>
                    <texturefocus border="20" colordiffuse="BackgroundColor">controls/circle-triangleup.png</texturefocus>
                    <onup>$PARAM[onup]</onup>
                    <ondown>$PARAM[ondown]</ondown>
                    <onleft>$PARAM[downbuttonid]</onleft>
                    <onright>$PARAM[onright]</onright>
                    <onclick>Control.Move($PARAM[controlid],-1)</onclick>
                </control>
            </control>
        </definition>
	</include>
            
	<include name="OrbitCircle">
        <param name="posx" default="0" />
        <param name="posy" default="0" />
        <param name="size" default="60" />
        <param name="color" default="MainColor" />
        <param name="radius" default="280" />
        <param name="time" default="10000" />
        <param name="delay" default="0" />
        <param name="direction" default="true" />

        <definition>
            <control type="group">
                <visible>$PARAM[direction]</visible>
                <animation effect="rotate" center="$PARAM[posx],$PARAM[posy]" start="360" end="0" time="$PARAM[time]" delay="$PARAM[delay]" loop="true" condition="true">Conditional</animation>
                <posx>$PARAM[posx]</posx>
                <posy>$PARAM[posy]</posy>

                <control type="image">
                    <posy>$PARAM[radius]</posy>
                    <width>$PARAM[size]</width>
                    <height>$PARAM[size]</height>
                    <texture colordiffuse="$PARAM[color]">elements/circle-mask.png</texture>
                </control>
            </control>
            <control type="group">
                <visible>!$PARAM[direction]</visible>
                <animation effect="rotate" center="$PARAM[posx],$PARAM[posy]" start="0" end="360" time="$PARAM[time]" delay="$PARAM[delay]" loop="true" condition="true">Conditional</animation>
                <posx>$PARAM[posx]</posx>
                <posy>$PARAM[posy]</posy>

                <control type="image">
                    <posy>$PARAM[radius]</posy>
                    <width>$PARAM[size]</width>
                    <height>$PARAM[size]</height>
                    <texture colordiffuse="$PARAM[color]">elements/circle-mask.png</texture>
                </control>
            </control>
        </definition>
	</include>
