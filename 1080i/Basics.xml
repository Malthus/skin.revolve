    <include name="UpDownButton">
        <param name="posx" default="0" />
        <param name="posy" default="0" />
        <param name="downbuttonid" default="-" />
        <param name="upbuttonid" default="-" />
        <param name="controlid" default="-" />
        <param name="onup" default="Noop" />
        <param name="ondown" default="Noop" />
        <param name="onleft" default="Noop" />
        <param name="onright" default="Noop" />

        <definition>
            <control type="group">
                <posx>$PARAM[posx]</posx>
                <posy>$PARAM[posy]</posy>

                <control type="button" id="$PARAM[downbuttonid]">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>40</width>
                    <height>40</height>
                    <texturenofocus border="20" colordiffuse="$VAR[SecondaryColor]">controls/circle-triangledown.png</texturenofocus>
                    <texturefocus border="20" colordiffuse="$VAR[BackgroundColor]">controls/circle-triangledown.png</texturefocus>
                    <onup>$PARAM[onup]</onup>
                    <ondown>$PARAM[ondown]</ondown>
                    <onleft>$PARAM[onleft]</onleft>
                    <onright>$PARAM[upbuttonid]</onright>
                    <onclick>Control.Move($PARAM[controlid],1)</onclick>
                </control>
                
                <control type="button" id="$PARAM[upbuttonid]">
                    <posx>40</posx>
                    <posy>0</posy>
                    <width>40</width>
                    <height>40</height>
                    <texturenofocus border="20" colordiffuse="$VAR[SecondaryColor]">controls/circle-triangleup.png</texturenofocus>
                    <texturefocus border="20" colordiffuse="$VAR[BackgroundColor]">controls/circle-triangleup.png</texturefocus>
                    <onup>$PARAM[onup]</onup>
                    <ondown>$PARAM[ondown]</ondown>
                    <onleft>$PARAM[downbuttonid]</onleft>
                    <onright>$PARAM[onright]</onright>
                    <onclick>Control.Move($PARAM[controlid],-1)</onclick>
                </control>
            </control>
        </definition>
    </include>

    <include name="ColorLabel">
        <param name="visible" default="true" />
        <param name="posx" default="0" />
        <param name="posy" default="0" />
        <param name="color" default="-" />
        <param name="buttonid" default="0" />

        <definition>
            <control type="group">
                <visible>$PARAM[visible]</visible>
                <posx>$PARAM[posx]</posx>
                <posy>$PARAM[posy]</posy>
                <control type="image">
                    <posx>5</posx>
                    <posy>5</posy>
                    <width>30</width>
                    <height>30</height>
                    <texture>elements/circle-mask.png</texture>
                    <colordiffuse>$PARAM[color]</colordiffuse>
                </control>
                <control type="image">
                    <width>40</width>
                    <visible>!Control.HasFocus($PARAM[buttonid])</visible>
                    <height>40</height>
                    <texture colordiffuse="$VAR[SecondaryColor]" border="20">controls/circle-clear.png</texture>
                </control>
                <control type="image">
                    <visible>Control.HasFocus($PARAM[buttonid])</visible>
                    <width>40</width>
                    <height>40</height>
                    <texture colordiffuse="$VAR[BackgroundColor]" border="20">controls/circle-clear.png</texture>
                </control>
            </control>
        </definition>
    </include>
    
