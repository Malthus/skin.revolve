<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="ItemForDebug">
        <param name="textcolor" default="White" />
        <param name="label" default="-" />
    
        <definition>
            <height>20</height>
            <include name="OutlineLabel">
                <param name="width" value="780" />
                <param name="height" value="20" />
                <param name="font" value="DebugFont" />
                <param name="textcolor" value="$PARAM[textcolor]" />
                <param name="outlinecolor" value="BackgroundColor" />
                <param name="outlinesize" value="2" />
                <param name="align" value="left" />
                <param name="label" value="$PARAM[label]" />
            </include>
        </definition>
	</include>

	<include name="ContentTypeForDebug">
        <param name="label" default="-" />
    
        <definition>
            <include name="ItemForDebug">
                <param name="textcolor" value="Red" />
                <param name="label" value="$PARAM[label]" />
            </include>
        </definition>
	</include>

	<include name="WindowNameForDebug">
        <param name="label" default="-" />
    
        <definition>
            <include name="ItemForDebug">
                <param name="textcolor" value="Blue" />
                <param name="label" value="$PARAM[label]" />
            </include>
        </definition>
	</include>

	<include name="DialogNameForDebug">
        <param name="label" default="-" />
    
        <definition>
            <include name="ItemForDebug">
                <param name="textcolor" value="Green" />
                <param name="label" value="$PARAM[label]" />
            </include>
        </definition>
	</include>
</includes>
