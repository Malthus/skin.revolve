<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="VisibilityForSquareIcon">
        <definition>
            <visible>!Container.Content(tvshows) + !Container.Content(movies) + !Container.Content(seasons)</visible>
        </definition>
    </include>
    
    <include name="VisibilityForVerticalIcon">
        <definition>
            <visible>Container.Content(tvshows) | Container.Content(movies) | Container.Content(seasons)</visible>
        </definition>
    </include>

    <include name="VisibilityForHorizontalIcon">
        <definition>
            <visible>false</visible>
        </definition>
    </include>
    
    <include name="VisibilityForBanner">
        <definition>
            <visible>!String.IsEmpty(ListItem.Art(banner))</visible>
        </definition>
    </include>
    
    <include name="VisibilityForNoBanner">
        <definition>
            <visible>String.IsEmpty(ListItem.Art(banner))</visible>
        </definition>
    </include>
    
    <include name="VisibilityForThumbnail">
        <definition>
            <visible>!String.IsEmpty(ListItem.Art(thumb)) | !String.IsEmpty(ListItem.Icon)</visible>
        </definition>
    </include>
    
    <include name="VisibilityForNoThumbnail">
        <definition>
            <visible>String.IsEmpty(ListItem.Art(thumb)) + String.IsEmpty(ListItem.Icon)</visible>
        </definition>
    </include>
    
