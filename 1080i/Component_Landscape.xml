	</include>
	
	<include name="OtherElementForLandscape">	
        <param name="texture" default="-" />
    
        <definition>
            <control type="group">
                <animation type="Conditional" condition="true">
                    <effect type="zoom" center="960,724" end="80" />
                    <effect type="rotate" center="auto" end="0" />
                </animation>
                <posx>795</posx>
                <posy>380</posy>
                <width>330</width>
                <height>646</height>
                <include name="ImageForOtherElementForLandscape">
                    <param name="texture" value="$PARAM[texture]" />
                </include>                
			</control>            
        </definition>
	</include>
    
