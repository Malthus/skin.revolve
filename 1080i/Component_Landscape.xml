                <control type="group">
                    <animation effect="rotate" center="960,3600" end="21" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(-3).Label)</visible>
                    <include name="OtherElementForLandscape">	
                        <param name="texture" value="$INFO[Container.ListItem(-3).Icon]" />
                    </include>
