	<include name="SelectedElementForLandscape">
        <definition>
            <control type="group">
                <include name="VisibilityForVerticalIcon" />

                <control type="image">
                    <posx>20</posx>
                    <posy>20</posy>
                    <width>290</width>
                    <height>424</height>
                    <texture colordiffuse="MainColor">elements/poster-mask.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
                    
                <control type="image">
                    <posx>26</posx>
                    <posy>26</posy>
                    <width>278</width>
                    <height>412</height>
                    <texture diffuse="elements/poster-mask.png" fallback="white.png" background="true">$INFO[Container.ListItem(0).Icon]</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
            </control>
            
            <control type="group">
                <include name="VisibilityForSquareIcon" />
                <posy>134</posy>
                
                <control type="image">
                    <posx>20</posx>
                    <posy>20</posy>
                    <width>290</width>
                    <height>290</height>
                    <texture colordiffuse="MainColor">elements/poster-mask.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
                        
                <control type="image">
                    <posx>26</posx>
                    <posy>26</posy>
                    <width>278</width>
                    <height>278</height>
                    <texture diffuse="elements/poster-mask.png" fallback="white.png" background="true">$INFO[Container.ListItem(0).Icon]</texture>
                    <aspectratio>stretch</aspectratio>
