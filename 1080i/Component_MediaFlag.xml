<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="StarRatingMediaFlag">
		<control type="image" description="Star Rating Flag">
			<width>180</width>
			<height>40</height>
			<aspectratio>keep</aspectratio>
			<texture>$VAR[ContentStarRating]</texture>
			<colordiffuse>MainColor</colordiffuse>
		</control>
	</include>

	<include name="DefaultOverlayMediaFlag" description="Transparant Default Flag Overlay">
		<control type="image">
			<width>200</width>
			<height>200</height>
			<texture colordiffuse="FadeDiffuseColor" background="true">$VAR[ContentOverlay]</texture>
			<aspectratio scalediffuse="false">keep</aspectratio>
		</control>
	</include>
	
	<include name="DefaultUnderlayMediaFlag" description="Transparant Default Flag Underlay">
		<control type="image">
			<width>200</width>
			<height>200</height>
			<texture colordiffuse="SelectColor" background="true">$VAR[ContentUnderlay]</texture>
			<aspectratio scalediffuse="false">keep</aspectratio>
		</control>
	</include>
	
	<include name="VideoCodecVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.VideoCodec)</visible>
	</include>

	<include name="VideoResolutionVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.VideoResolution)</visible>
	</include>

	<include name="VideoAspectRatioVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.VideoAspect)</visible>
	</include>

	<include name="VideoStereoscopicVisibilityForContentMediaFlag">
		<visible>ListItem.IsStereoscopic</visible>
	</include>
	
	<include name="AudioCodecVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.AudioCodec) | SubString(ListItem.FileExtension,ape) | SubString(ListItem.FileExtension,flac) | SubString(ListItem.FileExtension,mp3) | SubString(ListItem.FileExtension,wav) | SubString(ListItem.FileExtension,wma)</visible>
	</include>

	<include name="AudioChannelsVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.AudioChannels)</visible>
	</include>

	<include name="PictureCodecVisibilityForContentMediaFlag">
		<visible>SubString(ListItem.FileExtension,jpg) | SubString(ListItem.FileExtension,jpeg) | SubString(ListItem.FileExtension,png) | SubString(ListItem.FileExtension,gif)</visible>
	</include>

	<include name="PictureSizeVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.PictureResolution)</visible>
	</include>

	<include name="PictureISOVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.PictureISO)</visible>
	</include>

	<include name="PictureApertureVisibilityForContentMediaFlag">
		<visible>!IsEmpty(ListItem.PictureAperture)</visible>
	</include>

	<include name="MediumVisibilityForContentMediaFlag">
		<visible>SubString(ListItem.FileName,bluray) | SubString(ListItem.FileName,blu-ray) | SubString(ListItem.FileName,brrip) | SubString(ListItem.FileName,bd25) | SubString(ListItem.FileName,bd50) | SubString(ListItem.FileName,dvd) | SubString(Player.FileName,hdtv) | SubString(ListItem.FileName,pdtv) | SubString(ListItem.FileName,dsr) | SubString(ListItem.FileName,vhs)</visible>
	</include>

	<include name="CompressedPropertyVisibilityForContentMediaFlag">
		<visible>StringCompare(ListItem.Overlay,OverlayRAR.png) | StringCompare(ListItem.Overlay,OverlayZIP.png)</visible>
	</include>

	<include name="LockedPropertyVisibilityForContentMediaFlag">
		<visible>ListItem.IsEncrypted | StringCompare(ListItem.Overlay,OverlayLocked.png)</visible>
	</include>
	
	<include name="TrainerPropertyVisibilityForContentMediaFlag">
		<visible>StringCompare(ListItem.Overlay,OverlayTrainer.png) | StringCompare(ListItem.Overlay,OverlayHasTrainer.png)</visible>
	</include>
	
	<include name="EpgPropertyVisibilityForContentMediaFlag">
		<visible>ListItem.HasEpg</visible>
	</include>
	
	<include name="MpaaRatingVisibilityForContentMediaFlag">
		<visible>SubString(ListItem.Mpaa,rated)</visible>
	</include>

