        <control type="group">
            <visible>Player.Playing</visible>

            <control type="group" description="Play Icon">
                <posx>340</posx>
                <posy>1015</posy>

                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>40</width>
                    <height>40</height>
                    <texture colordiffuse="MainColor">elements/circle-mask.png</texture>
                </control>

                <control type="image">
                    <posx>10</posx>
                    <posy>8</posy>
                    <width>24</width>
                    <height>24</height>
                    <texture colordiffuse="BackgroundColor">icons/player-play.png</texture>
                </control>
            </control>
            
            <control type="group" description="Information Label">
                <posx>390</posx>
                <posy>1015</posy>
					
