	<include name="SideMenu">
        <definition>
            <posx>20</posx>
            <posy>100</posy>
		
            <animation type="conditional" condition="!ControlGroup(9000).HasFocus() + !ControlGroup(9001).HasFocus() + !ControlGroup(9010).HasFocus() + !ControlGroup(9020).HasFocus() + !Skin.HasSetting(SideMenu.Visible) + !Control.IsVisible(10) + !Control.IsVisible(53) + !Control.IsVisible(56) + !Control.IsVisible(57)">
                <effect type="slide" start="0,0" end="-610,0" time="200" />
            </animation>
            <animation type="conditional" condition="!ControlGroup(9000).HasFocus() + !ControlGroup(9001).HasFocus() + !ControlGroup(9010).HasFocus() + !ControlGroup(9020).HasFocus() + Skin.HasSetting(SideMenu.Visible) + !Control.IsVisible(10) + !Control.IsVisible(53) + !Control.IsVisible(56) + !Control.IsVisible(57)">
                <effect type="slide" start="0,0" end="-515,0" time="200" />
            </animation>
		
            <animation type="conditional" condition="!ControlGroup(9000).HasFocus() + !ControlGroup(9001).HasFocus() + !ControlGroup(9010).HasFocus() + !ControlGroup(9020).HasFocus() + !Skin.HasSetting(SideMenu.Visible) + [Control.IsVisible(10) | Control.IsVisible(53) | Control.IsVisible(56) | Control.IsVisible(57)]">
                <effect type="rotate" start="0" end="90" center="0,0" time="200" />
            </animation>
            <animation type="conditional" condition="!ControlGroup(9000).HasFocus() + !ControlGroup(9001).HasFocus() + !ControlGroup(9010).HasFocus() + !ControlGroup(9020).HasFocus() + Skin.HasSetting(SideMenu.Visible) + [Control.IsVisible(10) | Control.IsVisible(53) | Control.IsVisible(56) | Control.IsVisible(57)]">
                <effect type="rotate" start="0" end="90" center="0,45" time="200" />
            </animation>

            <control type="group">
                <control type="image">
                    <posx>-30</posx>
                    <posy>-30</posy>
                    <width>588</width>
                    <height>840</height>
                    <fadetime>100</fadetime>
                    <aspectratio>stretch</aspectratio>
                    <texture border="30" background="true">elements/poster-shadow.png</texture>
                </control>
			
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>528</width>
                    <height>780</height>
                    <fadetime>100</fadetime>
                    <aspectratio>stretch</aspectratio>
                    <texture colordiffuse="BackgroundColor" fallback="white.png">elements/poster-background.png</texture>
                </control>
            </control>
			
            <control type="group">
                <description>Side Menu Label When Side Menu Is Unfocused</description>
                <visible>Skin.HasSetting(SideMenu.Visible)</visible>

                <animation type="conditional" condition="!ControlGroup(9000).HasFocus() + !ControlGroup(9001).HasFocus() + !ControlGroup(9010).HasFocus() + Skin.HasSetting(SideMenu.Visible)">
                    <effect type="fade" start="0" end="100" time="200" />
                </animation>

                <control type="button" id="9020">
                    <description>Invisible Mouse Control Side Menu Button</description>
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>528</width>
                    <height>780</height>
                    <texturenofocus>-</texturenofocus>
                    <texturefocus>-</texturefocus>
                    <font>-</font>
                    <label>-</label>
                    <onfocus>Control.SetFocus(9001)</onfocus>
                </control>
			
                <control type="label">
                    <description>Left Side Menu Label</description>
                    <posx>112</posx>
                    <posy>400</posy>
                    <width>800</width>
                    <height>20</height>
                    <font>SideMenuLabelFont</font>
                    <textcolor>SelectColor</textcolor>
                    <align>center</align>
                    <angle>90</angle>
                    <label>[UPPERCASE]$LOCALIZE[31907][/UPPERCASE]</label>
                </control>
			
                <control type="label" id="9021">
                    <description>Top Side Menu Label</description>
                    <posx>-385</posx>
                    <posy>400</posy>
                    <width>800</width>
