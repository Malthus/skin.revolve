<?xml version="1.0" encoding="UTF-8"?>
<window id="1110">
    <defaultcontrol always="true">100</defaultcontrol>
    <onunload condition="!Window.Next(music) + !Window.Next(musicplaylist)">ClearProperty(ViewMode,music)</onunload>
    <onunload condition="!Window.Next(music) + !Window.Next(musicplaylist)">ClearProperty(ViewMode,musicplaylist)</onunload>

    <controls>
        <include content="GlobalBackground" />

        <include content="OutlineLabel">
            <param name="id" value="101" />
            <param name="posx" value="500" />
            <param name="posy" value="520" />
            <param name="width" value="920" />
            <param name="height" value="40" />
            <param name="font" value="SmallMessageFont" />
            <param name="textcolor" value="$VAR[MainColor]" />
            <param name="outlinecolor" value="$VAR[BackgroundColor]" />
            <param name="outlinesize" value="2" />
            <param name="align" value="center" />
            <param name="wrapmultiline" value="false" />
            <param name="label" value="$LOCALIZE[31007]" />
        </include>        

        <control type="button" id="100">
            <visible allowhiddenfocus="true">true</visible>
            <posx>950</posx>
            <posy>530</posy>
            <width>20</width>
            <height>20</height>
            <texturenofocus></texturenofocus>
            <texturefocus></texturefocus>
            <font></font>
            <label></label>
            <onup>ActivateWindow(musicosd)</onup>
            <ondown>Noop</ondown>
            <onleft>SetProperty(ViewMode,Jukebox,music)</onleft>
            <onleft>SetProperty(ViewMode,Jukebox,musicplaylist)</onleft>
            <onleft>ActivateWindow(musicplaylist)</onleft>
            <onright>SetProperty(ViewMode,Jukebox,music)</onright>
            <onright>SetProperty(ViewMode,Jukebox,musicplaylist)</onright>
            <onright>ActivateWindow(music,musicdb://albums/,return)</onright>
        </control>
    </controls>
</window>
