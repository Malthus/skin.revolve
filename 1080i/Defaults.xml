<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <default type="label">
        <include content="DefaultControlDimensions" />
        <font>DialogTextFont</font>
        <textcolor>$VAR[SecondaryColor]</textcolor>
        <focusedcolor>$VAR[MainColor]</focusedcolor>
        <selectedcolor>$VAR[HighlightColor]</selectedcolor> 
        <disabledcolor>$VAR[TextColor]</disabledcolor>
        <align>left</align>
        <aligny>center</aligny>
        <scroll>true</scroll>
        <scrollspeed>50</scrollspeed>
        <scrollsuffix> | </scrollsuffix>
        <label>-</label>
    </default>

    <default type="edit">
        <include content="DefaultControlDimensions" />
        <font>DialogTextFont</font>
        <textcolor>$VAR[MainColor]</textcolor>
        <focusedcolor>$VAR[BackgroundColor]</focusedcolor>
        <texturenofocus colordiffuse="$VAR[BackgroundColor]" border="20">controls/rectangle-fill.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[MainColor]" border="20">controls/rectangle-fill.png</texturefocus>
        <textoffsetx>20</textoffsetx>
        <align>center</align>
        <aligny>center</aligny>
        <label>-</label>
    </default>
    
    <default type="image">
        <width>-</width>
        <height>-</height>
        <align>center</align>
        <aligny>center</aligny>
        <texture>-</texture>
    </default>

    <default type="button">
        <include content="DefaultButtonDimensions" />
        <font>NormalButtonFont</font>
        <textoffsetx>20</textoffsetx>
        <textcolor>$VAR[MainColor]</textcolor>
        <focusedcolor>$VAR[BackgroundColor]</focusedcolor>
        <disabledcolor>$VAR[TextColor]</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <texturenofocus colordiffuse="$VAR[BackgroundColor]" border="20">controls/rectangle-fill.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[MainColor]" border="20">controls/rectangle-fill.png</texturefocus>
        <align>center</align>
        <aligny>center</aligny>
        <label>-</label>
    </default>

    <default type="togglebutton">
        <include content="DefaultButtonDimensions" />
        <font>NormalButtonFont</font>
        <textoffsetx>20</textoffsetx>
        <textcolor>$VAR[MainColor]</textcolor>
        <focusedcolor>$VAR[BackgroundColor]</focusedcolor>
        <disabledcolor>$VAR[TextColor]</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <texturenofocus colordiffuse="$VAR[BackgroundColor]" border="20">controls/rectangle-fill.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[MainColor]" border="20">controls/rectangle-fill.png</texturefocus>
        <alttexturenofocus colordiffuse="$VAR[BackgroundColor]" border="20">controls/rectangle-fill.png</alttexturenofocus>
        <alttexturefocus colordiffuse="$VAR[MainColor]" border="20">controls/rectangle-fill.png</alttexturefocus>
        <align>center</align>
        <aligny>center</aligny>
        <label>-</label>
    </default>

    <default type="radiobutton">
        <include content="DefaultButtonDimensions" />
        <font>NormalButtonFont</font>
        <textoffsetx>20</textoffsetx>
        <textcolor>$VAR[MainColor]</textcolor>
        <focusedcolor>$VAR[BackgroundColor]</focusedcolor>
        <disabledcolor>$VAR[TextColor]</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <texturenofocus colordiffuse="$VAR[BackgroundColor]" border="20">controls/rectangle-fill.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[MainColor]" border="20">controls/rectangle-fill.png</texturefocus>
        <radiowidth>40</radiowidth>
        <radioheight>40</radioheight>
        <textureradioonfocus colordiffuse="$VAR[BackgroundColor]">controls/circle-dot.png</textureradioonfocus>
        <textureradioofffocus colordiffuse="$VAR[BackgroundColor]">controls/circle-clear.png</textureradioofffocus>
        <textureradioonnofocus colordiffuse="$VAR[SecondaryColor]">controls/circle-dot.png</textureradioonnofocus>
        <textureradiooffnofocus colordiffuse="$VAR[SecondaryColor]">controls/circle-clear.png</textureradiooffnofocus>
        <textureradioondisabled colordiffuse="$VAR[TextColor]">controls/circle-dot.png</textureradioondisabled>
        <textureradiooffdisabled colordiffuse="$VAR[TextColor]">controls/circle-clear.png</textureradiooffdisabled>
        <align>center</align>
        <aligny>center</aligny>
        <label>-</label>
    </default>

    <default type="spincontrol">
        <posx>0</posx>
        <posy>0</posy>
        <height>40</height>
        <width>40</width>
        <spinwidth>40</spinwidth>
        <spinheight>40</spinheight>
        <textureup border="20" colordiffuse="$VAR[SecondaryColor]">controls/circle-triangleup.png</textureup>
        <texturedown border="20" colordiffuse="$VAR[SecondaryColor]">controls/circle-triangledown.png</texturedown>
        <textureupfocus border="20" colordiffuse="$VAR[BackgroundColor]">controls/circle-triangleup.png</textureupfocus>
        <texturedownfocus border="20" colordiffuse="$VAR[BackgroundColor]">controls/circle-triangledown.png</texturedownfocus>
        <textureupdisabled border="20" colordiffuse="$VAR[TextColor]">controls/circle-triangleup.png</textureupdisabled>
        <texturedowndisabled border="20" colordiffuse="$VAR[TextColor]">controls/circle-triangledown.png</texturedowndisabled>
        <font>NormalButtonFont</font>
        <textcolor>$VAR[MainColor]</textcolor>
        <focusedcolor>$VAR[BackgroundColor]</focusedcolor>
        <disabledcolor>$VAR[TextColor]</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="spincontrolex">
        <include content="DefaultButtonDimensions" />
        <font>NormalButtonFont</font>
        <textoffsetx>20</textoffsetx>
        <textcolor>$VAR[MainColor]</textcolor>
        <focusedcolor>$VAR[BackgroundColor]</focusedcolor>
        <disabledcolor>$VAR[TextColor]</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <texturenofocus colordiffuse="$VAR[BackgroundColor]" border="20">controls/rectangle-fill.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[MainColor]" border="20">controls/rectangle-fill.png</texturefocus>
        <spinwidth>38</spinwidth>
        <spinheight>38</spinheight>
        <textureup colordiffuse="$VAR[SecondaryColor]">controls/circle-triangleup.png</textureup>
        <texturedown colordiffuse="$VAR[SecondaryColor]">controls/circle-triangledown.png</texturedown>
        <textureupfocus colordiffuse="$VAR[BackgroundColor]">controls/circle-triangleup.png</textureupfocus>
        <texturedownfocus colordiffuse="$VAR[BackgroundColor]">controls/circle-triangledown.png</texturedownfocus>
        <textureupdisabled colordiffuse="$VAR[TextColor]">controls/circle-triangleup.png</textureupdisabled>
        <texturedowndisabled colordiffuse="$VAR[TextColor]">controls/circle-triangledown.png</texturedowndisabled>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <label>-</label>
    </default>

    <default type="slider">
        <posx>0</posx>
        <posy>0</posy>
        <width>346</width>
        <height>40</height>
        <texturefocus>-</texturefocus>
        <texturenofocus>-</texturenofocus>
        <texturesliderbar colordiffuse="$VAR[SecondaryColor]">controls/line-horizontal.png</texturesliderbar>
        <textureslidernib colordiffuse="$VAR[SecondaryColor]">controls/circle-verticalhole.png</textureslidernib>
        <textureslidernibfocus colordiffuse="$VAR[BackgroundColor]">controls/circle-verticalhole.png</textureslidernibfocus>
        <orientation>horizontal</orientation>
    </default>

    <default type="sliderex">
        <include content="DefaultButtonDimensions" />
        <sliderwidth>346</sliderwidth>
        <sliderheight>40</sliderheight>
        <texturesliderbar colordiffuse="$VAR[SecondaryColor]">controls/line-horizontal.png</texturesliderbar>
        <textureslidernib colordiffuse="$VAR[SecondaryColor]">controls/circle-verticalhole.png</textureslidernib>
        <textureslidernibfocus colordiffuse="$VAR[BackgroundColor]">controls/circle-verticalhole.png</textureslidernibfocus>
        <texturenofocus colordiffuse="$VAR[BackgroundColor]" border="20">controls/rectangle-fill.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[MainColor]" border="20">controls/rectangle-fill.png</texturefocus>
        <font>NormalButtonFont</font>
        <textcolor>$VAR[MainColor]</textcolor>
        <focusedcolor>$VAR[BackgroundColor]</focusedcolor>
        <disabledcolor>$VAR[TextColor]</disabledcolor>
        <align>left</align>
        <aligny>center</aligny>
        <textoffsetx>20</textoffsetx>
    </default>

    <default type="progress">
        <texturebg border="6">-</texturebg>
        <midtexture colordiffuse="$VAR[MainColor]" border="6">controls/horizontalbar-thin.png</midtexture>
        <lefttexture>-</lefttexture>
        <righttexture>-</righttexture>
        <overlaytexture>-</overlaytexture>
    </default>

    <default type="scrollbar">
        <width>20</width>
        <texturesliderbackground border="0,14,0,14" colordiffuse="$VAR[BackgroundColor]">controls/horizontalbar-thick.png</texturesliderbackground>
        <texturesliderbar border="0,14,0,14" colordiffuse="$VAR[SelectColor]">controls/horizontalbar-thin.png</texturesliderbar>
        <texturesliderbarfocus border="0,14,0,14" colordiffuse="$VAR[MainColor]">controls/horizontalbar-thin.png</texturesliderbarfocus>
        <showonepage>true</showonepage>
        <orientation>vertical</orientation>
    </default>

    <default type="textbox">
        <font>DescriptionViewFont</font>
        <textcolor>$VAR[SecondaryColor]</textcolor>
        <pagecontrol>-</pagecontrol>
        <autoscroll delay="3000" time="2000" repeat="5000"/>
        <label>-</label>
    </default>
</includes>
