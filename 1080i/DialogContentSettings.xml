<?xml version="1.0" encoding="UTF-8"?>
<window id="132">
	<defaultcontrol always="true">20</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>
		<control type="group">
			<posy>PosYForTenLinesDialog</posy>
			<include name="TenLinesDialog" />

            <include name="DefaultQuestionMarkForDialog" />
            
			<include name="HeaderForDialog">
				<param name="label" value="$LOCALIZE[20333]" />
			</include>
			
			<control type="spincontrolex" id="20">
                <description>Directory Content Option</description>
				<posx>560</posx>
				<posy>80</posy>
				<width>1240</width>
				<label>-</label>
				<onup>5</onup>
				<ondown>21</ondown>
				<onleft>9000</onleft>
				<onright>Noop</onright>
			</control>

			<control type="label">
                <description>Unfocused Directory Contains Label</description>
				<visible>!Control.HasFocus(20)</visible>
				<posx>580</posx>
				<posy>80</posy>
				<width>1200</width>
				<font>DialogTextFont</font>
				<textcolor>MainColor</textcolor>
				<label>$LOCALIZE[20344]</label>
			</control>

			<control type="label">
                <description>Focused Directory Contains Label</description>
				<visible>Control.HasFocus(20)</visible>
				<posx>580</posx>
				<posy>80</posy>
				<width>1200</width>
				<font>DialogTextFont</font>
				<textcolor>BackgroundColor</textcolor>
				<label>$LOCALIZE[20344]</label>
			</control>

			<control type="list" id="21">
                <description>List With Relevant Plugins</description>
				<posx>560</posx>
				<posy>140</posy>
				<width>1240</width>
				<height>140</height>
				<onup>20</onup>
				<onleft>9000</onleft>
				<onright>Noop</onright>
				<ondown>5</ondown>

				<itemlayout height="40">
					<include>NormalItemsForNormalList</include>
				</itemlayout>
					
				<focusedlayout height="40">
					<control type="group">
						<visible>Control.HasFocus(21)</visible>
						<include>FocusedItemsForNormalList</include>
					</control>
					<control type="group">
						<visible>!Control.HasFocus(21)</visible>
						<include>NormalItemsForNormalList</include>
					</control>
				</focusedlayout>
			</control>

			<control type="grouplist" id="5">
				<posx>560</posx>
				<posy>320</posy>
				<width>1240</width>
				<height>160</height>
				<orientation>vertical</orientation>
				<itemgap>0</itemgap>
				<onup>21</onup>
				<ondown>20</ondown>
				<onleft>9000</onleft>
				<onright>Noop</onright>
			</control>

            <include name="DefaultControlsForDialog" />
            
			<control type="grouplist" id="9000">
                <include name="MarkupForButtonListForDialog">
                    <param name="posy" value="320" />
                    <param name="height" value="160" />
                </include>
				<onright>5</onright>

                <control type="button" id="22">
                    <description>Settings Button</description>
                    <label>$LOCALIZE[5]</label>
                </control>
                <control type="button" id="28">
                    <description>Ok Button</description>
                    <posy>40</posy>
                    <label>$LOCALIZE[186]</label>
                </control>
                <control type="button" id="29">
                    <description>Cancel Button</description>
                    <label>$LOCALIZE[222]</label>
