<?xml version="1.0" encoding="UTF-8"?>
<window id="142">
	<defaultcontrol always="true">603</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	
	<controls>
		<control type="group">
			<control type="group">
				<animation type="WindowOpen">
					<effect type="fade" start="0" end="100" time="500" />
				</animation>
				<animation type="WindowClose">
					<effect type="fade" start="100" end="0" time="500" />
				</animation>

				<control type="group">
					<visible>Skin.HasSetting(MediaIcons.Visible)</visible>
					<posx>340</posx>
					<posy>870</posy>
					<include>HorizontalListOfVideoPlayerMediaFlag</include>
				</control>
				
				<control type="group">
					<include>MediaInformationLabelForPlayer</include>
					<include>MediaNameLabelForPlayer</include>
					<include>MediaSeekBarForPlayer</include>
				</control>
			</control>

			<control type="group">
				<animation type="WindowOpen">
					<effect type="slide" tween="quadratic" easing="out" start="1590,0" end="0,0" time="500" />
				</animation>
				<animation type="WindowClose">
					<effect type="slide" tween="quadratic" easing="out" start="0,0" end="1590,0" time="500" />
				</animation>

				<control type="group">
					<posx>40</posx>
					<posy>806</posy>
					<include>PlayerMediaIconForPlayer</include>
				</control>
			</control>
		</control>
	</controls>
</window>
