    <controls>
        <control type="group">
            <posx>1840</posx>
            <posy>PosYForLowerIconForOverlay</posy>

            <include content="PlayerSpeedFlag">
                <param name="flagcolor" value="$VAR[BackgroundColor]" />
                <param name="backgroundcolor" value="$VAR[MainColor]" />
            </include>
        </control>
    </controls>
</window>
