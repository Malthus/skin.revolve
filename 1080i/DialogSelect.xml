<?xml version="1.0" encoding="UTF-8"?>
<window id="2000">
	<defaultcontrol always="true">3</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>
		<control type="group">
			<posy>PosYForThirteenLinesDialog</posy>
			<include>ThirteenLinesDialog</include>

			<control type="group" description="Dialog Thumbnail Icon">
				<posx>PosXForDefaultIconFrameForDialog</posx>
				<posy>PosYForDefaultIconFrameForDialog</posy>
				<include>DefaultCircleQuestionThumbnailIcon</include>
			</control>

			<control type="label" id="1" description="Selection Header Label">
				<posx>560</posx>
				<posy>20</posy>
				<width>1240</width>
				<font>DialogTitleFont</font>
				<textcolor>SecondaryColor</textcolor>
				<align>left</align>
				<label>13406</label>
			</control>

			<control type="list" id="3" description="Normal Selection List">
				<posx>560</posx>
				<posy>80</posy>
				<width>1240</width>
				<height>520</height>
				<onleft>5</onleft>
				<onright>Noop</onright>
				<onup>3</onup>
				<ondown>3</ondown>

				<itemlayout height="40">
					<include>NormalItemsForNormalList</include>
				</itemlayout>
					
				<focusedlayout height="40">
					<control type="group">
						<visible>Control.HasFocus(3)</visible>
						<include>FocusedItemsForNormalList</include>
					</control>
					<control type="group">
						<visible>!Control.HasFocus(3)</visible>
						<include>SelectedItemsForNormalList</include>
					</control>
				</focusedlayout>
			</control>

			<control type="list" id="6" description="Selection List With Thumbnails For Addons">
				<posx>560</posx>
				<posy>80</posy>
				<width>1240</width>
				<height>520</height>
				<onleft>5</onleft>
				<onright>Noop</onright>
				<onup>6</onup>
				<ondown>6</ondown>
				
				<itemlayout height="100">
					<include>NormalItemsForLargeThumbnailList</include>
				</itemlayout>
					
				<focusedlayout height="100">
					<control type="group">
						<visible>Control.HasFocus(6)</visible>
						<include>FocusedItemsForLargeThumbnailList</include>
					</control>
					<control type="group">
						<visible>!Control.HasFocus(6)</visible>
						<include>SelectedItemsForLargeThumbnailList</include>
					</control>
				</focusedlayout>
			</control>

			<control type="label" id="2" description="Number Of Files Label">
				<posx>40</posx>
				<posy>520</posy>
				<width>480</width>
				<font>DialogTextFont</font>
				<textcolor>SecondaryColor</textcolor>
				<align>center</align>
				<label>-</label>
			</control>

			<control type="button" id="5" description="Manual Search/Get More Button">
				<posx>40</posx>
				<posy>560</posy>
				<label>368</label>
				<onleft>Noop</onleft>
				<onright>3</onright>
				<onup>5</onup>
				<ondown>5</ondown>
			</control>
		</control>
	</controls>
</window>
