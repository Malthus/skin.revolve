<?xml version="1.0" encoding="UTF-8"?>
<window id="126" type="dialog">
	<defaultcontrol always="true">450</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>
		<control type="group">
			<posy>PosYForFifteenLinesDialog</posy>
			<include>FifteenLinesDialog</include>

			<control type="group" description="Dialog Icon (thumbnail of selected item)">
				<posx>150</posx>
				<posy>20</posy>
				<include>DefaultCircleThumbnailIcon</include>
			</control>

			<control type="label" id="411" description="File Browser Header Label">
				<posx>560</posx>
				<posy>20</posy>
				<width>1240</width>
				<label>1023</label>
				<font>DialogTitleFont</font>
				<textcolor>SecondaryColor</textcolor>
				<align>left</align>
			</control>

			<control type="label" id="412" description="Path Label">
				<posx>560</posx>
				<posy>80</posy>
				<width>1240</width>
				<haspath>true</haspath>
			</control>

			<control type="list" id="450">
				<posx>560</posx>
				<posy>120</posy>
				<width>1240</width>
				<height>560</height>
				<pagecontrol>451</pagecontrol>
				<onup>450</onup>
				<ondown>450</ondown>
				<onleft>9000</onleft>
				<onright>451</onright>

                <itemlayout height="100">
                    <include>NormalItemsForLargeThumbnailList</include>
                </itemlayout>
					
                <focusedlayout height="100">
                    <control type="group">
                        <visible>Control.HasFocus(450)</visible>
                        <include>FocusedItemsForLargeThumbnailList</include>
                    </control>
                    <control type="group">
                        <visible>!Control.HasFocus(450)</visible>
                        <include>SelectedItemsForLargeThumbnailList</include>
                    </control>
                </focusedlayout>

			</control>

			<control type="scrollbar" id="451" description="File List Scrollbar">
				<posy>120</posy>
				<height>560</height>
				<include>MarkupForScrollbarForDialog</include>
				<onleft>450</onleft>
				<onright>Noop</onright>
			</control>
            
			<control type="grouplist" id="9000" description="Button Area">
				<posx>40</posx>
				<posy>520</posy>
				<itemgap>0</itemgap>
				<orientation>vertical</orientation>
				<onup>9000</onup>
				<ondown>9000</ondown>
				<onright>450</onright>
				<onleft>Noop</onleft>

				<control type="button" id="413" description="Ok Button">
					<label>186</label>
				</control>

				<control type="button" id="414" description="Cancel Button">
					<label>222</label>
				</control>

				<control type="button" id="415" description="Create Folder Button">
					<label>119</label>
				</control>

				<control type="radiobutton" id="416" description="Horizontal Flip Button">
					<label>749</label>
				</control>
			</control>
		</control>
	</controls>
</window>
