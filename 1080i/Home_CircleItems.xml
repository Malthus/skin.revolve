<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="HomeCircleItems">
		<control type="group">
			<control type="fixedlist" id="600">
                <visible allowhiddenfocus="true">Control.HasFocus(600)</visible>
                <description>Items In Circle List</description>
                <animation effect="fade" start="0" end="100" time="500">Visible</animation>
                <animation effect="fade" start="100" end="0" time="500">Hidden</animation>
                
				<posx>-100</posx>
				<posy>330</posy>
				<width>812</width>
				<height>662</height>
				<onup>600</onup>
				<ondown>600</ondown>
				<onleft>Noop</onleft>
				<onright>300</onright>
				<scrolltime>0</scrolltime>
				
				<itemlayout width="800" height="0">
				</itemlayout>
				
				<focusedlayout width="800" height="622">
                    <control type="group">
                        <description>Movies In Circle List</description>
                        <visible allowhiddenfocus="true">Container(300).HasFocus(5)</visible>
                        
                        <control type="image">
                            <posx>66</posx>
                            <posy>185</posy>
                            <width>527</width>
                            <height>296</height>
                            <texture background="true" diffuse="circle/movies-mask.png" fallback="black.png">$INFO[ListItem.Icon]</texture>
                            <aspectratio scalediffuse="false">stretch</aspectratio>
                        </control>
		
                        <control type="image">
                            <width>662</width>
                            <height>662</height>
                            <texture background="true">circle/movies-focus.png</texture>
                        </control>
                    </control>
			
                    <control type="group">
                        <description>TV Shows In Circle List</description>
                        <visible allowhiddenfocus="true">Container(300).HasFocus(6)</visible>

                        <control type="image">
                            <posx>38</posx>
                            <posy>68</posy>
                            <width>486</width>
                            <height>447</height>
                            <texture background="true" diffuse="circle/tvshows-mask.png" fallback="black.png">$INFO[ListItem.Icon]</texture>
                            <aspectratio scalediffuse="false">scale</aspectratio>
                        </control>
		
                        <control type="image">
                            <width>662</width>
                            <height>662</height>
                            <texture background="true">circle/tvshows-focus.png</texture>
                        </control>
                    </control>

                    <control type="group">
                        <description>Albums In Circle List</description>
                        <visible allowhiddenfocus="true">Container(300).HasFocus(16)</visible>
                        
                        <control type="image">
                            <width>662</width>
                            <height>662</height>
                            <texture background="true">circle/albums-focus.png</texture>
                        </control>
                        
                        <control type="image">
                            <posx>118</posx>
                            <posy>58</posy>
                            <width>468</width>
                            <height>468</height>
                            <texture diffuse="circle/albums-mask.png" fallback="black.png" background="true">$INFO[ListItem.Icon]</texture>
                            <aspectratio scalediffuse="false">scale</aspectratio>
                        </control>
                    </control>

                    <control type="label">
                        <posx>120</posx>
                        <posy>550</posy>
                        <width>812</width>
                        <height>40</height>
                        <font>MediumViewFont</font>
                        <textcolor>BackgroundColor</textcolor>
                        <align>left</align>
                        <scroll>true</scroll>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
			
                    <control type="label">
                        <posx>120</posx>
                        <posy>590</posy>
                        <width>802</width>
                        <height>30</height>
                        <font>HomeMenuSpecialFont</font>
                        <textcolor>BackgroundColor</textcolor>
                        <align>left</align>
                        <scroll>true</scroll>
                        <label>[UPPERCASE]$INFO[ListItem.Label2][/UPPERCASE]</label>
                    </control>
				</focusedlayout>
				
				<content>
					<include>ListOfHomeCircleItems</include>
				</content>
			</control>
		</control>
	</include>
	
