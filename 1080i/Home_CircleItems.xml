                        <control type="image">
                            <width>662</width>
                            <height>662</height>
                            <texture background="true">circle/albums-focus.png</texture>
                        </control>
                        
                        <control type="image">
                            <posx>118</posx>
                            <posy>58</posy>
                            <width>468</width>
                            <height>468</height>
                            <texture diffuse="circle/albums-mask.png" fallback="black.png" background="true">$INFO[ListItem.Icon]</texture>
                            <aspectratio scalediffuse="false">scale</aspectratio>
                        </control>
                    </control>

                    <control type="label">
                        <posx>120</posx>
                        <posy>550</posy>
                        <width>800</width>
                        <height>40</height>
                        <font>MediumViewFont</font>
                        <textcolor>White</textcolor>
                        <align>left</align>
                        <aligny>top</aligny>
                        <wrapmultiline>true</wrapmultiline>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
			
                    <control type="label">
                        <posx>120</posx>
                        <posy>590</posy>
                        <width>800</width>
                        <height>30</height>
                        <font>HomeMenuSpecialFont</font>
                        <textcolor>White</textcolor>
                        <align>left</align>
                        <label>[UPPERCASE]$INFO[ListItem.Label2][/UPPERCASE]</label>
                    </control>
				</focusedlayout>
				
				<content>
					<include>ListOfHomeCircleItems</include>
				</content>
