<?xml version="1.0" encoding="UTF-8"?>
<window id="123">
	<defaultcontrol always="true">5</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>
		<control type="group">
			<visible>!Window.IsVisible(sliderdialog)</visible>
			<posy>PosYForThirteenLinesDialog</posy>
			<include>ThirteenLinesDialog</include>

			<control type="group" description="Smart Playlist Dialog Icon">
				<posx>PosXForDefaultIconFrameForDialog</posx>
				<posy>PosYForDefaultIconFrameForDialog</posy>
				<include>BackgroundForDefaultCircleThumbnailIcon</include>
				
				<control type="image">
				    <posx>3</posx>
				    <posy>3</posy>
					<width>256</width>
					<height>256</height>
					<texture colordiffuse="BackgroundColor">icons/icon-projector.png</texture>
					<aspectratio>scale</aspectratio>
				</control>
			</control>
			
			<control type="label" id="2" description="Settings Header Label">
				<posx>560</posx>
				<posy>20</posy>
				<width>1240</width>
				<font>DialogTitleFont</font>
				<align>left</align>
				<label>-</label>
			</control>

			<control type="grouplist" id="5" description="Control Area">
				<posx>560</posx>
				<posy>80</posy>
				<width>1240</width>
				<height>520</height>
				<itemgap>0</itemgap>
				<orientation>vertical</orientation>
				<onleft>Noop</onleft>
				<onright>Noop</onright>
				<onup>5</onup>
				<ondown>5</ondown>
			</control>

			<control type="button" id="7" description="Default Button">
				<align>left</align>
			</control>

			<control type="radiobutton" id="8" description="Default RadioButton">
				<align>left</align>
			</control>

			<control type="spincontrolex" id="9" description="Default spincontrolex">
				<align>left</align>
				<reverse>yes</reverse>
			</control>

			<control type="sliderex" id="13" description="Default Slider">
				<align>left</align>
			</control>
		</control>
	</controls>
</window>
