<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="BannersView">
		<control type="panel" id="59" description="Panels">
			<viewtype label="31939">list</viewtype>
			<visible>true</visible>
			<animation effect="slide" start="0,1080" end="0,0" time="500">WindowOpen</animation>
			<animation effect="slide" start="0,0" end="0,1080" time="500">WindowClose</animation>
			
			<posx>190</posx>
			<posy>0</posy>
			<width>1600</width>
			<height>1000</height>
			<orientation>vertical</orientation>
			<defaultcontrol>6</defaultcontrol>
			<focusposition>6</focusposition>
			<scrolltime>200</scrolltime>
			<pagecontrol>69</pagecontrol>
			<preloaditems>2</preloaditems>
			<onup>59</onup>
			<ondown>59</ondown>
			<onleft>9000</onleft>
			<onright>69</onright>

			<itemlayout width="800" height="152">
				<control type="group">
					<posx>18</posx>
					<posy>3</posy>
					<include>DefaultBannerIcon</include>
<!--
					<include>OverlayForDefaultSquareThumbnailIcon</include>
-->                    
				</control>
			</itemlayout>
			
			<focusedlayout width="800" height="152">
				<control type="group">
					<animation effect="zoom" start="100" end="120" time="100" condition="true">conditional</animation>
					<animation effect="slide" end="-60,-12" time="100" condition="true">conditional</animation>				
			
					<posx>18</posx>
					<posy>3</posy>
					<include>ShadowForDefaultBannerIcon</include>
					<include>DefaultBannerIcon</include>
<!--                    
					<include>OverlayForDefaultSquareThumbnailIcon</include>
-->                    
				</control>
			</focusedlayout>
		</control>
        
		<control type="group" description="Bottom Ring">
			<visible>Control.IsVisible(59)</visible>
			<include>BottomRing</include>
		</control>
        
		<control type="scrollbar" id="69" description="Scrollbar">
			<visible allowhiddenfocus="true">Control.IsVisible(59)</visible>
			<animation type="Conditional" condition="!Control.HasFocus(69) + !Container(59).Scrolling + !Skin.HasSetting(ViewScrollbar.Visible)">
				<effect type="slide" start="0,0" end="40,0" time="100" />
			</animation>

			<posx>1880</posx>
			<posy>100</posy>
			<height>760</height>
			<onleft>59</onleft>
			<onright>Noop</onright>
		</control>
	</include>
</includes>
