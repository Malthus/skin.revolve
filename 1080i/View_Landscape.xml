	<include name="MainElementForLandscapeView">
        <definition>
            <control type="group">
                <include name="VisibilityForVerticalIcon" />

                <control type="image">
                    <posx>20</posx>
                    <posy>20</posy>
                    <width>290</width>
                    <height>424</height>
                    <texture colordiffuse="MainColor">elements/poster-mask.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
                    
                <control type="image">
                    <posx>26</posx>
                    <posy>26</posy>
                    <width>278</width>
                    <height>412</height>
                    <texture diffuse="elements/poster-mask.png" fallback="white.png" background="true">$INFO[Container.ListItem(0).Icon]</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
            </control>
            
            <control type="group">
                <include name="VisibilityForSquareIcon" />
                <posy>134</posy>
                
                <control type="image">
                    <posx>20</posx>
                    <posy>20</posy>
                    <width>290</width>
                    <height>290</height>
                    <texture colordiffuse="MainColor">elements/poster-mask.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
                        
                <control type="image">
                    <posx>26</posx>
                    <posy>26</posy>
                    <width>278</width>
                    <height>278</height>
                    <texture diffuse="elements/poster-mask.png" fallback="white.png" background="true">$INFO[Container.ListItem(0).Icon]</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
            </control>
        </definition>
	</include>
	
	<include name="OtherElementForLandscapeView">	
        <param name="texture" default="-" />
    
        <definition>
            <control type="group">
                <animation type="Conditional" condition="true">
                    <effect type="zoom" center="960,724" end="80" />
                    <effect type="rotate" center="auto" end="0" />
                </animation>
                <posx>795</posx>
                <posy>380</posy>
                <width>330</width>
                <height>646</height>
                <include name="ImageForOtherElementForLandscapeView">
                    <param name="texture" value="$PARAM[texture]" />
                </include>                
			</control>            
        </definition>
	</include>
    
	<include name="ImageForOtherElementForLandscapeView">	
        <param name="texture" default="-" />
    
        <definition>
            <control type="group">
                <include name="VisibilityForVerticalIcon" />

                <control type="image">
                    <posx>20</posx>
                    <posy>20</posy>
                    <width>290</width>
                    <height>424</height>
                    <texture colordiffuse="MainColor">elements/poster-mask.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>
                        
                <control type="image">
