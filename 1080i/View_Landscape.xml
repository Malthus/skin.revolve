                        <visible>Container.OnPrevious</visible>
                        <animation effect="zoom" center="960,724" start="100" end="125" time="200" reversible="false">Hidden</animation>
                        <animation type="Conditional" condition="true">
                            <effect type="zoom" center="960,724" end="80" />
                        </animation>
                        <posx>795</posx>
                        <posy>380</posy>
                        <width>330</width>
                        <height>646</height>

                        <control type="group">
                            <animation effect="fade" start="0" end="100" time="200" reversible="false">Hidden</animation>
                            <include name="ShadowForElementForLandscapeView" />
                        </control>
                        
                        <include name="MainElementForLandscapeView" />
                    </control>
                </control>
            
                <control type="group">
                    <visible>!IsEmpty(Container.ListItemNoWrap(1).Label)</visible>
                                
                    <control type="group">
                        <visible>Container.OnPrevious</visible>
                        <animation effect="zoom" center="960,724" start="100" end="80" time="200" reversible="false">Hidden</animation>
                        <posx>795</posx>
                        <posy>380</posy>
                        <width>330</width>
                        <height>646</height>

                        <control type="group">
                            <animation effect="fade" start="100" end="0" time="200" reversible="false">Hidden</animation>
                            <include name="ShadowForElementForLandscapeView" />
                        </control>
                        
                        <include name="ImageForOtherElementForLandscapeView">
                            <param name="texture" value="$INFO[Container.ListItem(1).Icon]" />
                        </include>
                    </control>
                </control>
            
                <control type="group">
                    <animation effect="rotate" center="960,3600" end="-7" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(2).Label)</visible>
                    <include name="OtherElementForLandscapeView">	
                        <param name="texture" value="$INFO[Container.ListItem(2).Icon]" />
                    </include>
                </control>
            
                <control type="group">
                    <animation effect="rotate" center="960,3600" end="-14" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(3).Label)</visible>
                    <include name="OtherElementForLandscapeView">	
                        <param name="texture" value="$INFO[Container.ListItem(3).Icon]" />
                    </include>
                </control>
            
