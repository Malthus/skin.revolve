                    <include name="OtherElementForLandscapeView">	
                        <param name="texture" value="$INFO[Container.ListItem(3).Icon]" />
                    </include>
                </control>
            </control>
            
            <control type="group" id="301">
                <visible>Control.IsVisible(53) + Container(53).OnNext</visible>
                <animation type="Hidden" reversible="false">
                    <effect type="rotate" center="960,3600" start="0" end="5" time="200" />
                </animation>
                
                <control type="group">
                    <animation effect="rotate" center="960,3600" end="21" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(-4).Label)</visible>
                    <include name="OtherElementForLandscapeView">	
                        <param name="texture" value="$INFO[Container.ListItem(-4).Icon]" />
                    </include>
                </control>
            
                <control type="group">
