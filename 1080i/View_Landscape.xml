                <visible>Control.IsVisible(53) + Container(53).OnPrevious</visible>
                <animation type="Hidden" reversible="false">
                    <effect type="rotate" center="960,3600" start="0" end="-5" time="200" />
                </animation>
                    
                <control type="group">
                    <animation effect="rotate" center="960,3600" end="21" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(-2).Label)</visible>
