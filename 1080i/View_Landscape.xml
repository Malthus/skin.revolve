<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="LandscapeView">
		<control type="group">
			<visible>Control.IsVisible(53)</visible>
			<include name="BackgroundAnimationForBottomRing" />
		</control>
        
		<control type="fixedlist" id="53">
			<viewtype label="31933">list</viewtype>
			<visible allowhiddenfocus="true">true</visible>
			
			<posx>0</posx>
			<posy>0</posy>
			<width>1920</width>
			<height>1080</height>
			<pagecontrol>63</pagecontrol>
			<orientation>horizontal</orientation>
			<focusposition>2</focusposition>
			<onup>9000</onup>
			<ondown>63</ondown>
			<onleft>53</onleft>
			<onright>53</onright>
			
            <include name="InvisibleHorizontalListItemsForMenu" />
		</control>
		
		<include name="MainLandscapeView" />
	</include>
		
	<include name="MainLandscapeView">
        <definition>
            <control type="group">
                <visible>Control.IsVisible(53)</visible>
                <animation effect="slide" start="0,1080" end="0,0" time="500">WindowOpen</animation>
                <animation effect="slide" start="0,0" end="0,1080" time="500">WindowClose</animation>

                <include name="BottomRing" />
                <include name="ItemsForLandscapeView" />
                <include name="ContentDescriptionForLandscapeView" />
            </control>

            <control type="scrollbar" id="63">
                <visible allowhiddenfocus="true">Control.IsVisible(53)</visible>
                <animation type="Conditional" condition="!Control.HasFocus(63) + !Container(53).Scrolling + !Skin.HasSetting(ViewScrollbar.Visible)">
                    <effect type="slide" start="0,0" end="0,-40" time="100" />
                </animation>

                <posx>PosXForHorizontalScrollbarForView</posx>
                <posy>PosYForHorizontalScrollbarForView</posy>
                <width>WidthForHorizontalScrollbarForView</width>
                <height>HeightForHorizontalScrollbarForView</height>
                <orientation>horizontal</orientation>
                <texturesliderbackground border="14,0,14,0" colordiffuse="BackgroundColor">controls/verticalbar-thick.png</texturesliderbackground>
                <texturesliderbar border="14,0,14,0" colordiffuse="SelectColor">controls/verticalbar-thin.png</texturesliderbar>
                <texturesliderbarfocus border="14,0,14,0" colordiffuse="MainColor">controls/verticalbar-thin.png</texturesliderbarfocus>
                <onup>53</onup>
                <ondown>53</ondown>
                <onleft>63</onleft>
                <onright>63</onright>
            </control>
        </definition>
	</include>
    
	<include name="ContentDescriptionForLandscapeView">
        <definition>
            <control type="group">
                <visible>Skin.HasSetting(Landscape.DescriptionVisible)</visible>
                <include name="OutlineTextbox">
                    <param name="posx" value="40" />
                    <param name="posy" value="40" />
                    <param name="width" value="1840" />
                    <param name="height" value="340" />
                    <param name="font" value="DescriptionViewFont" />
                    <param name="textcolor" value="BackgroundColor" />
                    <param name="outlinecolor" value="TextColor" />
                    <param name="outlinesize" value="2" />
                    <param name="label" value="$VAR[ContentDescription]" />
                </include>
            </control>
        </definition>
	</include>
    
	<include name="ItemsForLandscapeView">
        <definition>
            <control type="group" id="303">
                <visible>Control.IsVisible(53)</visible>
                <animation effect="fade" start="0" end="100" delay ="195" time="5" reversible="false" condition="Control.IsVisible(301) | Control.IsVisible(302)">Conditional</animation>

                <control type="group">
                    <animation effect="rotate" center="960,3600" end="21" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(-3).Label)</visible>
                    <include name="OtherElementForLandscapeView">	
                        <param name="texture" value="$INFO[Container.ListItem(-3).Icon]" />
                    </include>
                </control>
            
                <control type="group">
                    <animation effect="rotate" center="960,3600" end="14" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(-2).Label)</visible>
                    <include name="OtherElementForLandscapeView">	
                        <param name="texture" value="$INFO[Container.ListItem(-2).Icon]" />
                    </include>
                </control>
    
                <control type="group">
                    <animation effect="rotate" center="960,3600" end="7" condition="true">Conditional</animation>
                    <visible>!IsEmpty(Container.ListItemNoWrap(-1).Label)</visible>
                    <include name="OtherElementForLandscapeView">	
                        <param name="texture" value="$INFO[Container.ListItem(-1).Icon]" />
                    </include>
                </control>
            
                <control type="group">
                    <control type="group">
                        <posx>795</posx>
                        <posy>380</posy>
                        <width>330</width>
                        <height>646</height>
                
                        <control type="group">
                            <include name="ShadowForElementForLandscapeView" />
                        </control>
                        
