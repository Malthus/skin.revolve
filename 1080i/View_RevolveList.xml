                <control type="group">
                    <animation effect="rotate" tween="quadratic" easing="out" center="0,773" start="180" end="360" time="1000" reversible="false">WindowOpen</animation>
                    <animation effect="rotate" tween="quadratic" easing="in" center="0,773" start="360" end="180" time="1000" reversible="false">WindowClose</animation>
                    <animation effect="fade" start="100" end="30" time="500" condition="!Control.HasFocus(54)">Conditional</animation>
                    <include name="ItemsForRevolveListView" />
                </control>
                
                <include name="LeftRing" />
            </control>

            <control type="scrollbar" id="64">
                <visible allowhiddenfocus="true">Control.IsVisible(54)</visible>
                <animation type="Conditional" condition="!Control.HasFocus(64) + !Container(54).Scrolling + !Skin.HasSetting(ViewScrollbar.Visible)">
                    <effect type="slide" start="0,0" end="40,0" time="100" />
                </animation>

                <posx>PosXForVerticalScrollbarForView</posx>
                <posy>PosYForVerticalScrollbarForView</posy>
                <width>WidthForVerticalScrollbarForView</width>
                <height>HeightForVerticalScrollbarForView</height>
                <onleft>54</onleft>
                <onright>Noop</onright>
            </control>
        </definition>
