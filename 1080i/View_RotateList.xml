			<posx>1</posx>
			<posy>1</posy>
			<width>880</width>
			<height>30</height>
			<font>DetailViewFont</font>
			<textcolor>BackgroundColor</textcolor>
			<align>right</align>
			<label>$VAR[ContentInformationLabel]</label>
		</control>
	</include>

	<include name="NormalListItemMarkupForRotateListView">
		<posx>0</posx>
		<posy>160</posy>
		<height>40</height>
		<width>900</width>
		<font>MediumViewFont</font>
		<align>right</align>
		<scroll>false</scroll>
	</include>

	<include name="NoMovementAnimationForRotateListView">
		<visible>!Container(58).OnPrevious + !Container(58).OnNext</visible>
		<animation effect="fade" start="0" end="100" time="0" delay="200" reversible="false">Visible</animation>
	</include>
	
	<include name="PreviousAnimationForRotateListView">
		<visible>Container(58).OnPrevious</visible>
		<animation effect="rotate" center="1100,200" end="-8">Visible</animation>
		<animation effect="rotate" center="1100,200" start="-8" end="0" time="200">Hidden</animation>
	</include>
	
	<include name="NextAnimationForRotateListView">
		<visible>Container(58).OnNext</visible>
		<animation effect="rotate" center="1100,200" end="8">Visible</animation>
		<animation effect="rotate" center="1100,200" start="8" end="0" time="200">Hidden</animation>
	</include>
</includes>
