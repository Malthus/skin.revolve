<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="JukeboxMusicNavWindow">
        <definition>
            <control type="group">
                <animation effect="slide" start="1920,0" end="0,0" time="500" condition="!Window.Previous(musicplaylist)">WindowOpen</animation>
                <animation effect="slide" start="-1920,0" end="0,0" time="500" condition="Window.Previous(musicplaylist)">WindowOpen</animation>
                <animation effect="slide" start="0,0" end="1920,0" time="500" condition="!Window.Next(musicplaylist)">WindowClose</animation>
                <animation effect="slide" start="0,0" end="-1920,0" time="500" condition="Window.Next(musicplaylist)">WindowClose</animation>

                <include content="ThumbnailsPanelForThumbnailsView">
                    <param name="onleft" value="Close" />
                </include>
            </control>

            <control type="group">
                <visible>Control.IsVisible(55)</visible>

                <include content="VerticalScrollbarForView">
                    <param name="id" value="65" />
                    <param name="viewid" value="55" />
                    <param name="onright" value="ReplaceWindow(musicplaylist)" />
                </include>
            </control>

            <include content="GlobalOverlay">
                <param name="buttonid" value="200" />
                <param name="controlid" value="55" />
            </include>
        </definition>
    </include>
</includes>
