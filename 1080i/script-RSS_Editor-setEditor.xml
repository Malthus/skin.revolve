<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>18</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	
	<controls>
		<control type="group">
			<posy>PosYForThirteenLinesDialog</posy>
			<include>ThirteenLinesDialog</include>

			<control type="group" description="Editor Dialog Icon">
				<posx>PosXForBusyIconFrame</posx>
				<posy>PosYForBusyIconFrame</posy>
				<include>BusyIconFrame</include>

				<control type="image">
					<posx>4</posx>
					<posy>4</posy>
					<width>262</width>
					<height>262</height>
					<texture diffuse="elements/circle-mask.png" background="true">logos/rsseditor.png</texture>
					<aspectratio>keep</aspectratio>
				</control>
			</control>

			<control type="label" id="2" description="Header Label">
				<posx>560</posx>
				<posy>20</posy>
				<width>1240</width>
				<height>40</height>
				<font>DialogTitleFont</font>
				<textcolor>SecondaryColor</textcolor>
				<align>left</align>
				<label>-</label>
			</control>
			
			<control type="label" id="3" description="Instruction Label">
				<posx>560</posx>
				<posy>80</posy>
				<width>1240</width>
				<height>40</height>
				<font>DialogTextFont</font>
				<textcolor>TextColor</textcolor>
				<align>left</align>
				<label>-</label>
			</control>
			
			<control type="list" id="10" description="Control Area">
				<posx>560</posx>
				<posy>120</posy>
				<width>1240</width>
				<height>440</height>
				<orientation>vertical</orientation>
				<itemgap>0</itemgap>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<onleft>9000</onleft>
				<onright>60</onright>
				
				<itemlayout height="40">
					<include>NormalItemsForNormalList</include>
				</itemlayout>
					
				<focusedlayout height="40">
					<control type="group">
						<visible>Control.HasFocus(10)</visible>
						<include>FocusedItemsForNormalList</include>
					</control>
					<control type="group">
						<visible>!Control.HasFocus(10)</visible>
						<include>SelectedItemsForNormalList</include>
					</control>
				</focusedlayout>
			</control>
			
			<control type="scrollbar" id="60" description="Artist Information Scrollbar">
				<visible>Control.IsVisible(10)</visible>
				<posy>120</posy>
				<height>440</height>
				<include>MarkupForScrollbarForDialog</include>
				<onleft>10</onleft>
				<onright>Noop</onright>
			</control>

			<control type="grouplist" id="9000">
				<posx>40</posx>
				<posy>360</posy>
				<orientation>vertical</orientation>
				<usecontrolcoords>true</usecontrolcoords>
				<itemgap>0</itemgap>
				<onup>9000</onup>
				<ondown>9000</ondown>
				<onleft>Noop</onleft>
				<onright>10</onright>

				<control type="button" id="11" description="Change Set Button">
					<label>-</label>
				</control>
				
				<control type="button" id="13" description="Add Button">
					<label>$LOCALIZE[15019]</label>
				</control>
				
				<control type="button" id="14" description="Remove Button">
					<label>$LOCALIZE[1210]</label>
				</control>
				
				<control type="button" id="18" description="Ok Button">
					<posy>40</posy>
					<label>$LOCALIZE[186]</label>
				</control>
				
				<control type="button" id="19" description="Cancel Button">
					<label>$LOCALIZE[222]</label>
				</control>
			</control>
		</control>
	</controls>
</window>
