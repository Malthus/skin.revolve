<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
  <name>Last Century Albums</name>
  <match>all</match>
  <rule field="originalyear" operator="lessthan">
    <value>2000</value>
  </rule>
  <order direction="descending">random</order>
  <limit>10</limit>
</smartplaylist>
