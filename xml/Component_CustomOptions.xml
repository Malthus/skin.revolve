<?xml version="1.0" encoding="UTF-8"?>
<includes>
  <variable name="CommandForCustomOptions">
    <value condition="String.IsEqual(Window.Property(CustomType),W)">$INFO[Window.Property(CustomWindow),ActivateWindow(,)]</value>
    <value condition="String.IsEqual(Window.Property(CustomType),M)">ActivateWindow(1103)</value>
    <value condition="String.IsEqual(Window.Property(CustomType),P)">ActivateWindow(1102)</value>
    <value condition="String.IsEqual(Window.Property(CustomType),A)">$INFO[Window.Property(CustomAddon),RunScript(,)]</value>
    <value condition="String.IsEqual(Window.Property(CustomType),E)">RunScript(script.revolve.service,ExecuteShellCommand,"$INFO[Window.Property(CustomExecutable)]" $INFO[Window.Property(CustomParameters)])</value>
<!--    <value condition="String.IsEqual(Window.Property(CustomType),E)">System.Exec('"$INFO[Window.Property(CustomExecutable)]" $INFO[Window.Property(CustomParameters)]')</value> -->
    <value condition="String.IsEqual(Window.Property(CustomType),C)">$INFO[Control.GetLabel(48)]</value>
