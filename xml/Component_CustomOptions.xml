      </include>
      <include content="ActionForCustomOptions">
        <param name="label" value="Action: Run External Program" />
        <param name="visible" value="String.IsEqual(Window.Property(CustomType),E)" />
      </include>
      <include content="ActionForCustomOptions">
        <param name="label" value="Action: Execute $LOCALIZE[31805]" />
        <param name="visible" value="String.IsEqual(Window.Property(CustomType),C)" />
      </include>

      <control type="button" id="34">
        <description>Next Predefined Option Type Button</description>
        <posx>1700</posx>
        <posy>360</posy>
        <width>40</width>
        <height>40</height>
        <texturenofocus colordiffuse="$VAR[SecondaryColor]">controls/circle-triangledown.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[BackgroundColor]">controls/circle-triangledown.png</texturefocus>
        <onup>32</onup>
        <ondown>41</ondown>
        <onleft>9000</onleft>
        <onright>35</onright>
        <onclick condition="String.IsEqual(Window.Property(CustomType),C)">SetProperty(CustomType,W)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),E)">SetProperty(CustomType,C)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),A)">SetProperty(CustomType,E)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),P)">SetProperty(CustomType,A)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),M)">SetProperty(CustomType,P)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),W) + $PARAM[submenuenable]">SetProperty(CustomType,M)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),W) + !$PARAM[submenuenable]">SetProperty(CustomType,P)</onclick>
      </control>
      
      <control type="button" id="35">
        <description>Previous Predefined Option Type Button</description>
        <posx>1740</posx>
        <posy>360</posy>
        <width>40</width>
        <height>40</height>
        <texturenofocus colordiffuse="$VAR[SecondaryColor]">controls/circle-triangleup.png</texturenofocus>
        <texturefocus colordiffuse="$VAR[BackgroundColor]">controls/circle-triangleup.png</texturefocus>
        <onup>32</onup>
        <ondown>41</ondown>
        <onleft>34</onleft>
        <onright>Noop</onright>
        <onclick condition="String.IsEqual(Window.Property(CustomType),W)">SetProperty(CustomType,C)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),M)">SetProperty(CustomType,W)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),P) + $PARAM[submenuenable]">SetProperty(CustomType,M)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),P) + !$PARAM[submenuenable]">SetProperty(CustomType,W)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),A)">SetProperty(CustomType,P)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),E)">SetProperty(CustomType,A)</onclick>
        <onclick condition="String.IsEqual(Window.Property(CustomType),C)">SetProperty(CustomType,E)</onclick>
      </control>

      <control type="button" id="41">
        <description>Enter Submenu Identifier Button</description>
        <visible>String.IsEqual(Window.Property(CustomType),M)</visible>
        <posx>560</posx>
        <posy>400</posy>
        <width>1240</width>
        <font>DialogTextFont</font>
        <align>left</align>
        <label>$LOCALIZE[31896]: $INFO[Window.Property(CustomMenuIdentifier)]</label>
        <onup>34</onup>
