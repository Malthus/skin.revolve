        <onclick>SetProperty(TargetOption,$INFO[Window.Property(CustomMenuIdentifier)],1192)</onclick>
        <onclick>RunScript(script.revolve.service,PopulateSubmenuFromSkinVariables,$INFO[Window.Property(CustomMenuIdentifier)]%02dOption,CustomSubmenu%02dOption,1192)</onclick>
        <onclick>ActivateWindow(1192)</onclick>
        <onclick>SetProperty(CustomAction,ActivateWindow(1103))</onclick>
      </control>

      <!-- https://xbmc.github.io/docs.kodi.tv/master/kodi-base/d0/d3e/page__list_of_built_in_functions.html -->

      <!-- Open Window ~ selection (43) > Set Window + ActivateWindow > Action-property -->

      <!-- Open Playlist ~ select playlist (44) > CustomContentPath-property -->
      <control type="button" id="44">
        <description>Select Playlist Button</description>
        <visible>String.IsEqual(Window.Property(CustomType),P)</visible>
        <posx>560</posx>
        <posy>400</posy>
        <width>1240</width>
        <font>DialogTextFont</font>
        <align>left</align>
        <label>Playlist: $INFO[Window.Property(CustomContentPath)]</label>
        <onup>42</onup>
        <ondown>46</ondown>
        <onleft>9000</onleft>
        <onright>Noop</onright>
        <onclick>Skin.SetFile(Dialog.Input,,special://profile/playlists/)</onclick>
        <onclick>SetProperty(CustomContentPath,$INFO[Skin.String(Dialog.Input)])</onclick>
        <onclick>Skin.Reset(Dialog.Input)</onclick>
      </control>

      <!-- Run Add-on ~ select add-on type (45) > Property "xbmc.addon.executable" to set next control -->

      <!-- Run Add-on ~ select add-on (46) > RunScript > Action-property -->
      <control type="button" id="46">
        <description>Select Add-on Button</description>
        <visible>String.IsEqual(Window.Property(CustomType),A)</visible>
        <posx>560</posx>
        <posy>400</posy>
        <width>1240</width>
        <font>DialogTextFont</font>
        <align>left</align>
        <label>Add-on: $INFO[Window.Property(CustomAddon)]</label>
        <onup>44</onup>
        <ondown>47</ondown>
        <onleft>9000</onleft>
        <onright>Noop</onright>
        <onclick>Skin.SetAddon(Dialog.Input,xbmc.addon.executable)</onclick>
        <onclick>SetProperty(CustomAddon,$INFO[Skin.String(Dialog.Input)])</onclick>
        <onclick>Skin.Reset(Dialog.Input)</onclick>
      </control>

      <!-- Run External Program ~ select program (47) > CustomShellCommand -->
      <control type="button" id="47">
        <description>Select External Executable Button</description>
        <visible>String.IsEqual(Window.Property(CustomType),E)</visible>
        <posx>560</posx>
        <posy>400</posy>
        <width>1240</width>
        <font>DialogTextFont</font>
