          <onclick>ClearProperty(CustomOptionActive)</onclick>
          <onclick>ClearProperty(CustomName)</onclick>
          <onclick>ClearProperty(CustomSubtitle)</onclick>
          <onclick>ClearProperty(CustomBackgroundImage)</onclick>
