  <include name="ActionForCustomOptions">
    <param name="posx" default="580" />
    <param name="posy" default="360" />
    <param name="label" default="" />
    <param name="visible" default="true" />
    <param name="upid" default="34" />
    <param name="downid" default="35" />

    <definition>
      <control type="group">
        <visible>$PARAM[visible]</visible>
        <posx>$PARAM[posx]</posx>
        <posy>$PARAM[posy]</posy>

        <control type="label">
          <visible>!Control.HasFocus($PARAM[upid]) + !Control.HasFocus($PARAM[downid])</visible>
          <width>1100</width>
          <font>DialogTextFont</font>
          <textcolor>$VAR[MainColor]</textcolor>
          <align>left</align>
