        <onclick>SetProperty(CustomMenuIdentifier,$INFO[Skin.String(Dialog.Input)])</onclick>
        <onclick>SetProperty(CustomAction,ActivateWindow(1103))</onclick>
        <onclick>Skin.Reset(Dialog.Input)</onclick>
      </control>
      
      <control type="button" id="42">
