          <onclick>ClearProperty(CustomParameters)</onclick>
          <onclick>ClearProperty(CustomCommand)</onclick>
          <onclick>ClearProperty(CustomAction)</onclick>
        </control>
        <control type="radiobutton" id="26">
          <description>Help Radiobutton</description>
          <label>$LOCALIZE[10043]</label>
          <selected>Skin.HasSetting(DialogHelp.Visible)</selected>
          <onclick>Skin.ToggleSetting(DialogHelp.Visible)</onclick>
        </control>
        <control type="button" id="20">
          <description>Ok Button</description>
          <posy>20</posy>
          <label>$LOCALIZE[186]</label>
          <onclick condition="!String.IsEqual(Window.Property(CustomOptionActive),True)">Skin.Reset($INFO[Window.Property(TargetOption)].Active)</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomOptionActive),True)">Skin.SetBool($INFO[Window.Property(TargetOption)].Active)</onclick>
          <onclick>Skin.SetString($INFO[Window.Property(TargetOption)].Type,$INFO[Window.Property(CustomType)])</onclick>
          <onclick>Skin.SetString($INFO[Window.Property(TargetOption)].Name,$INFO[Window.Property(CustomName)])</onclick>
          <onclick>Skin.SetString($INFO[Window.Property(TargetOption)].Subtitle,$INFO[Window.Property(CustomSubtitle)])</onclick>
          <onclick>Skin.SetString($INFO[Window.Property(TargetOption)].BackgroundImage,$INFO[Window.Property(CustomBackgroundImage)])</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),W)">Skin.SetString($INFO[Window.Property(TargetOption)].Window,$INFO[Window.Property(CustomWindow)])</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),M)">Skin.SetString($INFO[Window.Property(TargetOption)].Window,1103)</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),P)">Skin.SetString($INFO[Window.Property(TargetOption)].Window,1102)</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),A) | String.IsEqual(Window.Property(CustomType),E) | String.IsEqual(Window.Property(CustomType),C)">Skin.SetString($INFO[Window.Property(TargetOption)].Window,0)</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),M)">Skin.SetString($INFO[Window.Property(TargetOption)].MenuIdentifier,$INFO[Window.Property(CustomMenuIdentifier)])</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),P)">Skin.SetString($INFO[Window.Property(TargetOption)].ContentPath,$INFO[Window.Property(CustomContentPath)])</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),A)">Skin.SetString($INFO[Window.Property(TargetOption)].Addon,$INFO[Window.Property(CustomAddon)])</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),E)">Skin.SetString($INFO[Window.Property(TargetOption)].Executable,$INFO[Window.Property(CustomExecutable)])</onclick>
          <onclick condition="String.IsEqual(Window.Property(CustomType),E)">Skin.SetString($INFO[Window.Property(TargetOption)].Parameters,$INFO[Window.Property(CustomParameters)])</onclick>
          <onclick>Skin.SetString($INFO[Window.Property(TargetOption)].Action,$VAR[CommandForCustomOptions])</onclick>
          <onclick>Close</onclick>
        </control>
        <control type="button" id="21">
          <description>Cancel Button</description>
          <label>$LOCALIZE[222]</label>
          <onclick>Close</onclick>
        </control>
      </control>
    </definition>
  </include>
  
