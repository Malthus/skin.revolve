        <param name="visible" value="!String.IsEqual(Window.Property(CustomType),M) + !String.IsEqual(Window.Property(CustomType),P) + !String.IsEqual(Window.Property(CustomType),A) + !String.IsEqual(Window.Property(CustomType),E) + !String.IsEqual(Window.Property(CustomType),C)" />
      </include>
      <include content="ActionForCustomOptions">
