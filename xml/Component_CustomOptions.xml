        <ondown>31</ondown>
        <onleft>9000</onleft>
        <onclick>Skin.SetString(Dialog.Input,$INFO[Window.Property(CustomName)])</onclick>
        <onclick>Skin.SetString(Dialog.Input)</onclick>
        <onclick>SetProperty(CustomName,$INFO[Skin.String(Dialog.Input)])</onclick>
        <onclick>Skin.Reset(Dialog.Input)</onclick>
      </control>

      <control type="button" id="31">
