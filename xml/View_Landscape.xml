<?xml version="1.0" encoding="UTF-8"?>
<includes>
  <include name="LandscapeView">
    <param name="listid" default="53" />
    <param name="scrollbarid" default="63" />
    <param name="sidemenuid" default="9001" />
    <param name="labelsvisible" default="true" />
    <param name="descriptionposx" default="40" />
    <param name="descriptionposy" default="60" />
    <param name="descriptionwidth" default="1840" />
    <param name="descriptionheight" default="320" />
    <param name="descriptionlabel" default="$VAR[ContentDescription]" />
    <param name="descriptionvisible" default="Skin.HasSetting(View.Landscape.DescriptionVisible)" />

    <definition>
      <control type="group">
        <visible>Control.IsVisible($PARAM[listid])</visible>
        <include content="BackgroundAnimationForBottomRing" />
      </control>

      <control type="fixedlist" id="$PARAM[listid]">
        <viewtype label="$LOCALIZE[31933]">list</viewtype>
        <visible allowhiddenfocus="true">true</visible>
        <left>-230</left>
        <top>0</top>
        <width>2380</width>
        <height>1000</height>
        <pagecontrol>$PARAM[scrollbarid]</pagecontrol>
        <orientation>horizontal</orientation>
        <focusposition>3</focusposition>
        <onup>$PARAM[sidemenuid]</onup>
        <ondown>$PARAM[scrollbarid]</ondown>
        <onleft>$PARAM[listid]</onleft>
        <onright>$PARAM[listid]</onright>

        <include content="InvisibleHorizontalListItemsForView">
          <param name="height" value="1000" />
          <param name="width" value="330" />
          <param name="focuswidth" value="400" />
        </include>
      </control>

      <control type="group">
        <visible>Control.IsVisible($PARAM[listid])</visible>
        <animation effect="slide" start="0,1080" end="0,0" time="500">WindowOpen</animation>
        <animation effect="slide" start="0,0" end="0,1080" time="500">WindowClose</animation>

        <include content="BottomRing" />
        
        <include content="HorizontalScrollbarForView">
          <param name="id" value="$PARAM[scrollbarid]" />
          <param name="viewid" value="$PARAM[listid]" />
        </include>

        <control type="group">
          <visible>$PARAM[labelsvisible]</visible>
          <include content="LabelsForBottomRing" />
        </control>

        <include content="ItemsForLandscapeView">
          <param name="listid" value="$PARAM[listid]" />
        </include>

        <control type="group">
          <visible>$PARAM[descriptionvisible]</visible>
          <include content="OutlineTextbox">
            <param name="posx" value="$PARAM[descriptionposx]" />
            <param name="posy" value="$PARAM[descriptionposy]" />
            <param name="width" value="$PARAM[descriptionwidth]" />
            <param name="height" value="$PARAM[descriptionheight]" />
            <param name="font" value="DescriptionViewFont" />
            <param name="textcolor" value="$VAR[BackgroundColor]" />
            <param name="outlinecolor" value="$VAR[TextColor]" />
            <param name="outlinesize" value="2" />
            <param name="label" value="$PARAM[descriptionlabel]" />
          </include>
        </control>
      </control>
    </definition>
  </include>

  <include name="ItemsForLandscapeView">
    <param name="listid" default="53" />

    <definition>
      <control type="group" id="303">
        <visible>Control.IsVisible($PARAM[listid])</visible>
        <animation effect="fade" start="0" end="100" delay ="195" time="5" reversible="false" condition="Control.IsVisible(301) | Control.IsVisible(302) | Container($PARAM[listid]).OnNext | Container($PARAM[listid]).OnPrevious">Conditional</animation>

        <control type="group">
          <animation effect="rotate" center="960,3600" end="21" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-3" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="14" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-2" />
          </include>
        </control>
  
        <control type="group">
          <animation effect="rotate" center="960,3600" end="7" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-1" />
          </include>
        </control>
      
        <control type="group">
          <control type="group">
            <left>795</left>
            <top>380</top>
            <width>330</width>
            <height>646</height>

            <include content="MainElementForLandscapeView">
              <param name="listid" value="$PARAM[listid]" />
            </include>

            <control type="group">
              <left>65</left>
              <top>226</top>
              <include content="ListItemOverlayFlag" />
            </control>
          </control>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="-7" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="1" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="-14" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="2" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="-21" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="3" />
          </include>
        </control>
      </control>

      <control type="group" id="301">
        <visible>Control.IsVisible($PARAM[listid]) + Container($PARAM[listid]).OnNext</visible>
        <animation type="Hidden" reversible="false">
          <effect type="rotate" center="960,3600" start="0" end="5" time="200" />
        </animation>

        <control type="group">
          <animation effect="rotate" center="960,3600" end="21" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-4" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="14" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-3" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="7" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-2" />
          </include>
        </control>

        <control type="group">
          <visible>!String.IsEmpty(Container($PARAM[listid]).ListItemNoWrap(-1).Label)</visible>

          <control type="group">
            <visible>Container($PARAM[listid]).OnNext</visible>
            <animation effect="zoom" center="960,724" start="100" end="80" time="200" reversible="false">Hidden</animation>
            <left>795</left>
            <top>380</top>
            <width>330</width>
            <height>646</height>

            <include content="IconForOtherElementForLandscapeView">
              <param name="listid" value="$PARAM[listid]" />
              <param name="listindex" value="-1" />
            </include>
          </control>
        </control>

        <control type="group">
          <animation effect="rotate" center="960,3600" end="-7" condition="true">Conditional</animation>
        
          <control type="group">
            <visible>Container($PARAM[listid]).OnNext</visible>
            <animation type="Conditional" condition="true">
              <effect type="zoom" center="960,724" end="80" />
            </animation>
            <animation effect="zoom" center="960,724" start="100" end="125" time="200" reversible="false">Hidden</animation>
            <left>795</left>
            <top>380</top>
            <width>330</width>
            <height>646</height>

            <include content="MainElementForLandscapeView">
              <param name="listid" value="$PARAM[listid]" />
            </include>
          </control>
        </control>

        <control type="group">
          <animation effect="rotate" center="960,3600" end="-14" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="1" />
          </include>
        </control>
  
        <control type="group">
          <animation effect="rotate" center="960,3600" end="-21" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="2" />
          </include>
        </control>
      </control>
      
      <control type="group" id="302">
        <visible>Control.IsVisible($PARAM[listid]) + Container($PARAM[listid]).OnPrevious</visible>
        <animation type="Hidden" reversible="false">
          <effect type="rotate" center="960,3600" start="0" end="-5" time="200" />
        </animation>
          
        <control type="group">
          <animation effect="rotate" center="960,3600" end="21" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-2" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="14" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="-1" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="7" condition="true">Conditional</animation>
        
          <control type="group">
            <visible>Container($PARAM[listid]).OnPrevious</visible>
            <animation effect="zoom" center="960,724" start="100" end="125" time="200" reversible="false">Hidden</animation>
            <animation type="Conditional" condition="true">
              <effect type="zoom" center="960,724" end="80" />
            </animation>
            <left>795</left>
            <top>380</top>
            <width>330</width>
            <height>646</height>

            <include content="MainElementForLandscapeView">
              <param name="listid" value="$PARAM[listid]" />
            </include>
          </control>
        </control>
      
        <control type="group">
          <visible>!String.IsEmpty(Container($PARAM[listid]).ListItemNoWrap(1).Label)</visible>

          <control type="group">
            <visible>Container($PARAM[listid]).OnPrevious</visible>
            <animation effect="zoom" center="960,724" start="100" end="80" time="200" reversible="false">Hidden</animation>
            <left>795</left>
            <top>380</top>
            <width>330</width>
            <height>646</height>

            <include content="IconForOtherElementForLandscapeView">
              <param name="listid" value="$PARAM[listid]" />
              <param name="listindex" value="1" />
            </include>
          </control>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="-7" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="2" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="-14" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="3" />
          </include>
        </control>
      
        <control type="group">
          <animation effect="rotate" center="960,3600" end="-21" condition="true">Conditional</animation>
          <include content="OtherElementForLandscapeView">  
            <param name="listid" value="$PARAM[listid]" />
            <param name="listindex" value="4" />
          </include>
        </control>
      </control>
    </definition>
  </include>

  <include name="MainElementForLandscapeView">
    <param name="listid" default="53" />

    <definition>
      <control type="group">
        <include content="VisibilityForVerticalIcon" />
        <left>20</left>
        <top>20</top>

        <include content="RectangleIcon">
          <param name="iconwidth" value="282" />
          <param name="iconheight" value="420" />
          <param name="iconoffset" value="4" />
          <param name="backgroundwidth" value="290" />
          <param name="backgroundheight" value="428" />
        </include>
      </control>
      
      <control type="group">
        <include content="VisibilityForSquareIcon" />
        <left>20</left>
        <top>154</top>
        
        <include content="RectangleIcon">
          <param name="iconwidth" value="282" />
          <param name="iconheight" value="282" />
          <param name="iconoffset" value="4" />
          <param name="backgroundwidth" value="290" />
          <param name="backgroundheight" value="290" />
        </include>
      </control>
    </definition>
  </include>
  
  <include name="OtherElementForLandscapeView">  
    <param name="listid" default="53" />
    <param name="listindex" default="0" />
  
    <definition>
      <control type="group">
        <visible>!String.IsEmpty(Container($PARAM[listid]).ListItemNoWrap($PARAM[listindex]).Label)</visible>
        <animation type="Conditional" condition="true">
          <effect type="zoom" center="960,724" end="80" />
          <effect type="rotate" center="auto" end="0" />
        </animation>
        <left>795</left>
        <top>380</top>
        <width>330</width>
        <height>646</height>
        <include content="IconForOtherElementForLandscapeView">
          <param name="listid" value="$PARAM[listid]" />
          <param name="listindex" value="$PARAM[listindex]" />
        </include>        
      </control>      
    </definition>
  </include>
  
  <include name="IconForOtherElementForLandscapeView">  
    <param name="listid" default="53" />
    <param name="listindex" default="0" />
  
    <definition>
      <control type="group">
        <include content="VisibilityForVerticalIcon" />

        <control type="image">
          <left>20</left>
          <top>20</top>
          <width>290</width>
          <height>424</height>
          <texture colordiffuse="$VAR[MainColor]">elements/poster-mask.png</texture>
          <aspectratio>stretch</aspectratio>
        </control>

        <include content="ImageForOtherElementForLandscapeView">  
          <param name="listindex" value="$PARAM[listindex]" />
          <param name="height" value="412" />
        </include>
      </control>

      <control type="group">
        <include content="VisibilityForSquareIcon" />

        <control type="image">
          <left>20</left>
          <top>154</top>
          <width>290</width>
          <height>290</height>
          <texture colordiffuse="$VAR[MainColor]">elements/poster-mask.png</texture>
          <aspectratio>stretch</aspectratio>
        </control>

        <include content="ImageForOtherElementForLandscapeView">  
          <param name="listindex" value="$PARAM[listindex]" />
          <param name="top" value="160" />
        </include>
      </control>
    </definition>
  </include>

  <include name="ImageForOtherElementForLandscapeView">  
    <param name="listindex" default="0" />
    <param name="left" default="26" />
    <param name="top" default="26" />
    <param name="width" default="278" />
    <param name="height" default="278" />

    <definition>
      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],-4)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapMinusFourThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>
      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],-3)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapMinusThreeThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>
      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],-2)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapMinusTwoThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>
      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],-1)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapMinusOneThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>

      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],0)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>

      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],1)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapPlusOneThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>
      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],2)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapPlusTwoThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>
      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],3)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapPlusThreeThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>
      <control type="image">
        <visible>Integer.IsEqual($PARAM[listindex],4)</visible>
        <left>$PARAM[left]</left>
        <top>$PARAM[top]</top>
        <width>$PARAM[width]</width>
        <height>$PARAM[height]</height>
        <texture condition="false" diffuse="elements/poster-mask.png" background="true">$VAR[ContentNoWrapPlusFourThumbnail]</texture>
        <aspectratio>stretch</aspectratio>
      </control>
    </definition>
  </include>
</includes>
